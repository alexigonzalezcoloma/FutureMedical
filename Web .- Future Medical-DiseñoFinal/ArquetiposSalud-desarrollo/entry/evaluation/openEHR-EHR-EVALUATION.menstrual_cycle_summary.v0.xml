  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Menstrual cycle summary</items>
        <items id="description">Summary of significant menstrual cycle events.</items>
      </items>
      <items code="at0001">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Menarche</items>
        <items id="description">Date of onset of first menstrual cycle. Partial dates, such as year only or year and month are allowed.</items>
      </items>
      <items code="at0003">
        <items id="text">Last normal menstrual period (LNMP)</items>
        <items id="description">Date of the onset of the last normal menstrual period. Useful to persist for calculations including estimated date of confinement in pregnancy, or to distinguish the latest typical menstrual cycle when there is an irregular menstrual pattern.</items>
      </items>
      <items code="at0004">
        <items id="text">Menopause</items>
        <items id="description">Onset of Menopause. Partial dates, such as year only or year and month are allowed.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Menstruasjonssyklus-sammendrag</items>
        <items id="description">Oversikt over viktige hendelser knyttet til relevante datoer om menstruasjon.</items>
      </items>
      <items code="at0001">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Menarke</items>
        <items id="description">Dato for første menstruasjon. Deldatoer, som år og måned eller bare år er tillatt.</items>
      </items>
      <items code="at0003">
        <items id="text">Siste normale menstruasjon</items>
        <items id="description">Dato for første dag av siste normale menstruasjon. Kan være nyttig for å utføre kalkulasjoner for bestemmelse av graviditet, eller for å skille mellom siste vanlige menstruasjon når det er et uregelmessig menstruasjonsmønster.</items>
      </items>
      <items code="at0004">
        <items id="text">Menopause</items>
        <items id="description">Datoen menopausen startet. Deldatoer, som år og måned eller bare år er tillatt.</items>
      </items>
    </term_definitions>
  </ontology>
