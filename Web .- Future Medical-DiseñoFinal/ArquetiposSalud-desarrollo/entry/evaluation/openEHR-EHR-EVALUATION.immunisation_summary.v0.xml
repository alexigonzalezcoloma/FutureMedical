  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Immunisation summary</items>
        <items id="description">Summary of the immunisation status for an identified infectious disease or agent.</items>
      </items>
      <items code="at0001">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Infectious disease or agent</items>
        <items id="description">Identification of the infectious disease or agent.</items>
        <items id="comment">There may be multiple diseases or agents that are vaccinated together - for example: diptheria, tetanus and pertussis or measles, mumps and rubella.</items>
      </items>
      <items code="at0003">
        <items id="text">Primary course status</items>
        <items id="description">Status of the primary course of immunisations.</items>
      </items>
      <items code="at0004">
        <items id="text">Not commenced</items>
        <items id="description">Primary course was not commenced.</items>
      </items>
      <items code="at0005">
        <items id="text">Incomplete</items>
        <items id="description">Primary course was commenced but not completed.</items>
      </items>
      <items code="at0006">
        <items id="text">Complete</items>
        <items id="description">Primary course was completed.</items>
      </items>
