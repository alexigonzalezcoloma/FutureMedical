  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Malnutrition screening tool (MST)</items>
        <items id="description">Screening for malnutrition tool.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal</items>
      </items>
      <items code="at0002">
        <items id="text">24h</items>
        <items id="description">It is recommended that nutrition screening  be performed within 24h of hospital admission.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Weight data</items>
        <items id="description">Have you recently lost weight without try?</items>
      </items>
      <items code="at0006">
        <items id="text">0: No</items>
        <items id="description">The patient no lost weight.</items>
      </items>
      <items code="at0007">
        <items id="text">2: Unsure</items>
        <items id="description">The patient is unsure about lost weight.</items>
      </items>
      <items code="at0008">
        <items id="text">Lost weight positive</items>
        <items id="description">If yes, how much weight have you lost?</items>
      </items>
      <items code="at0009">
        <items id="text">1: 1-5 kg.</items>
        <items id="description">Lost weight recent (1-5 kg).</items>
      </items>
      <items code="at0010">
        <items id="text">2: 6-10 kg</items>
        <items id="description">Lost weight recent (6-10 kg).</items>
      </items>
      <items code="at0011">
        <items id="text">3: 11-15 kg</items>
        <items id="description">Lost weight recent (11-15 kg).</items>
      </items>
      <items code="at0012">
        <items id="text">4: &gt; 15 kg</items>
        <items id="description">Lost weight recent (&gt; 15 kg).</items>
      </items>
      <items code="at0013">
        <items id="text">2: Unsure</items>
        <items id="description">The patient is unsure about kilograms of weight lost.</items>
      </items>
      <items code="at0016">
        <items id="text">Appetite</items>
        <items id="description">Have you been eating poorly because of a decreased appetite?</items>
      </items>
      <items code="at0017">
        <items id="text">0: No</items>
        <items id="description">The patient has not eaten poorly recently.</items>
      </items>
      <items code="at0018">
        <items id="text">1: Yes</items>
        <items id="description">The patient has eaten poorly recently.</items>
      </items>
      <items code="at0019">
        <items id="text">MST score</items>
        <items id="description">Score of 2 or more: patient at risk of malnutrition.</items>
      </items>
    </term_definitions>
  </ontology>
