  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Fitzpatrick skin type</items>
        <items id="description">Numerical schema for classifying skin colour type based on reaction to ultraviolet radiation exposure. </items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">*</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Skin type</items>
        <items id="description">The Fitzpatrick Skin type.</items>
      </items>
      <items code="at0005">
        <items id="text">I</items>
        <items id="description">Always burn, never tan</items>
      </items>
      <items code="at0006">
        <items id="text">II</items>
        <items id="description">Usually burn, tan less than average (with difficulty)</items>
      </items>
      <items code="at0007">
        <items id="text">III</items>
        <items id="description">Sometimes mild burn, tan about average</items>
      </items>
      <items code="at0008">
        <items id="text">IV</items>
        <items id="description">Rarely burn, tan more than average (with ease)</items>
      </items>
      <items code="at0009">
        <items id="text">V</items>
        <items id="description">Brown skin, rarely burns, tans profusely</items>
      </items>
      <items code="at0010">
        <items id="text">VI</items>
        <items id="description">Black skin, never burns</items>
      </items>
    </term_definitions>
  </ontology>
