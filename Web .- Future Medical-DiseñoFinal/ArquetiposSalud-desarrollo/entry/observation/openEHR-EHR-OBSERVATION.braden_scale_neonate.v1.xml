  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Neonatal Braden scale (NSRAS)</items>
        <items id="description">Neonatal Skin Risk Assessment Scale (NSRAS), an instrument to assess neonates at risk for skin breakdown, based on the Braden Scale for Predicting Pressure Sore Risk in adults.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Any event.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Graded risk</items>
        <items id="description">A graded risk derived from the summed Total score.</items>
      </items>
      <items code="at0005">
        <items id="text">Moisture</items>
        <items id="description">The dgree to which the infant's skin is exposed to moisture.</items>
      </items>
      <items code="at0006">
        <items id="text">Nutrition</items>
        <items id="description">The usual food intake of the neonate.</items>
      </items>
      <items code="at0007">
        <items id="text">Activity</items>
        <items id="description">The amount of physical activity of the neonate.</items>
      </items>
      <items code="at0008">
        <items id="text">Mobility</items>
        <items id="description">The neonate's ability ot change and control body position.</items>
      </items>
      <items code="at0009">
        <items id="text">Mental status</items>
        <items id="description">An evaluation of the mental status or sensory preception of the neonate.</items>
      </items>
      <items code="at0010">
        <items id="text">Total score</items>
        <items id="description">The total score, derived from the sum of all sub-scores for identifying if a neonate is at risk for pressure ulcers.</items>
      </items>
      <items code="at0011">
        <items id="text">General physical condition</items>
        <items id="description">An assessment of general condition, based on gestational age.</items>
      </items>
      <items code="at0012">
        <items id="text">Best</items>
        <items id="description">Gestational Age &gt; 38 Weeks To Posterm.</items>
      </items>
      <items code="at0013">
        <items id="text">Good</items>
        <items id="description">Gestational Age &gt; 33 Weeks But &lt; 38 weeks.</items>
      </items>
      <items code="at0014">
        <items id="text">Poor</items>
        <items id="description">Gestational Age &gt; 28 Weeks But &lt; 33 weeks.</items>
      </items>
      <items code="at0015">
        <items id="text">Very poor</items>
        <items id="description">Gestational Age &gt; 28 Weeks But &lt; 33 weeks.</items>
      </items>
      <items code="at0016">
        <items id="text">Comment</items>
        <items id="description">An additional comment about the neonatal pressure risk assessment.</items>
      </items>
      <items code="at0017">
        <items id="text">Rarely moist</items>
        <items id="description">Skin is usually dry, linen requires changing only every 24 hours.</items>
      </items>
      <items code="at0018">
        <items id="text">Occasionally moist</items>
        <items id="description">Skin is occasionally moist/damp. Requiring an extra linen change approximately once a day.</items>
      </items>
      <items code="at0019">
        <items id="text">Moist</items>
        <items id="description">Skin is often but not always moist/damp; linen must be changed at least once a shift.</items>
      </items>
      <items code="at0020">
        <items id="text">Constantly moist</items>
        <items id="description">Skin is moist/damp every time infant is moved or turned.</items>
      </items>
      <items code="at0021">
        <items id="text">Excellent</items>
        <items id="description">Bottlehreastfeeds every meal which  meets nutritional needs  for growth.</items>
      </items>
      <items code="at0022">
        <items id="text">Adequate</items>
        <items id="description">Is on tube feedings which  meet nutritional needs for growth.</items>
      </items>
      <items code="at0023">
        <items id="text">Inadequate</items>
        <items id="description">Receives less than  optimum amount of liquid diet for growth (formula/breast milk) and supplemented with intravenous fluids.</items>
      </items>
      <items code="at0024">
        <items id="text">Very poor</items>
        <items id="description">NPO on intravenous  fluids.</items>
      </items>
      <items code="at0025">
        <items id="text">Unlimited</items>
        <items id="description">In an open crib.</items>
      </items>
      <items code="at0026">
        <items id="text">Slightly limited</items>
        <items id="description">In a double walled isolette.</items>
      </items>
      <items code="at0027">
        <items id="text">Limited bed-bound</items>
        <items id="description">In a radiant warmer without a clear plastic “saran” tent.</items>
      </items>
      <items code="at0028">
        <items id="text">Completely bed-bound</items>
        <items id="description">In a radiant warmer with a clear plastic “saran” tent.</items>
      </items>
      <items code="at0029">
        <items id="text">No limitation</items>
        <items id="description">Makes major and frequent changes in position without assistance (e.g., turn  head).</items>
      </items>
      <items code="at0030">
        <items id="text">Slightly limited</items>
        <items id="description">Makes frequent though slight changes in body or extremity position independently.</items>
      </items>
      <items code="at0031">
        <items id="text">Very limited</items>
        <items id="description">Makes occasional slight changes in body or extremity position but unable to make frequent or significant changes independently.</items>
      </items>
      <items code="at0032">
        <items id="text">Completely immobile</items>
        <items id="description">Does not make even slight changes in body or extremity position without assistance (e.g., Pavulon).</items>
      </items>
      <items code="at0033">
        <items id="text">No impairment</items>
        <items id="description">Alert and active.</items>
      </items>
      <items code="at0034">
        <items id="text">Slightly limited</items>
        <items id="description">Lethargic.</items>
      </items>
      <items code="at0035">
        <items id="text">Very limited</items>
        <items id="description">Responds only to painful stimuli (flinches, grasps, moans, increased blood pressure or heart rate).</items>
      </items>
      <items code="at0036">
        <items id="text">Completely limited</items>
        <items id="description">Unresponsive (does not flinch, grasp, moan, increase blood pressure, or heart rate) to painful stimuli due to diminished level of consciousness or sedation.</items>
      </items>
      <items code="at0037">
        <items id="text">At risk - Total score 13 or over</items>
        <items id="description">The neonate is not at risk of developing a pressure ulcer.</items>
      </items>
      <items code="at0038">
        <items id="text">Not at risk - Total score less than 13</items>
        <items id="description">The neonate is at risk of developing a pressure ulcer.</items>
      </items>
    </term_definitions>
  </ontology>
