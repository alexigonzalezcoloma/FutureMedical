  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">TNM stage - pathological</items>
        <items id="description">A framework for the pathological classification and grading of malignancies using the TNM classification system.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Cancer type</items>
        <items id="description">The type of cancer being staged.</items>
        <items id="comment">Coding of the type of the cancer with a terminology is strongly preferred.</items>
      </items>
      <items code="at0006">
        <items id="text">Anatomical site</items>
        <items id="description">The anatomical site where the assessed cancer is situated.</items>
      </items>
      <items code="at0007">
        <items id="text">Anatomical subsite</items>
        <items id="description">The anatomical subsite where the assessed cancer is situated.</items>
      </items>
      <items code="at0008">
        <items id="text">Primary tumour (pT)</items>
        <items id="description">Pathological assessment of the primary tumour.</items>
      </items>
      <items code="at0009">
        <items id="text">Regional lymph nodes (pN)</items>
        <items id="description">Pathological assessment of the regional lymph nodes.</items>
        <items id="comment">May include suffixes. For example: sentinel nodes (sn); isolated tumor cells (ITCs) (i+); and FNA or a core biopsy (f).</items>
      </items>
      <items code="at0010">
        <items id="text">Distant metastasis (pM)</items>
        <items id="description">Pathological assessment of distant metastasis.</items>
      </items>
      <items code="at0011">
        <items id="text">Histopathological grading (G)</items>
        <items id="description">Histopathological grading of the tumour.</items>
      </items>
      <items code="at0012">
        <items id="text">Residual tumour (R)</items>
        <items id="description">Pathological assessment of post-treatment residual tumour.</items>
      </items>
      <items code="at0013">
        <items id="text">Lymphatic invasion (L)</items>
        <items id="description">Pathological assessment of invasion into the lymphatic system.</items>
      </items>
      <items code="at0014">
        <items id="text">Venous invasion (V)</items>
        <items id="description">Pathological assessment of invasion into the venous system.</items>
      </items>
      <items code="at0015">
        <items id="text">Other descriptor</items>
        <items id="description">Any other descriptor used in the pathological assessment.</items>
      </items>
      <items code="at0016">
        <items id="text">Stage</items>
        <items id="description">Concatenation of pT, pN and pM categories plus any of G, R, L and V, if applicable.</items>
      </items>
      <items code="at0017">
        <items id="text">Stage grouping</items>
        <items id="description">Pathological stage grouping.</items>
      </items>
      <items code="at0018">
        <items id="text">Retreatment (r)</items>
        <items id="description">Pathological staging is performed for a recurring cancer after a disease-free interval.</items>
      </items>
      <items code="at0019">
        <items id="text">Multimodality therapy (y)</items>
        <items id="description">Pathological staging is performed during or following initial multimodality therapy.</items>
      </items>
      <items code="at0020">
        <items id="text">Autopsy (a)</items>
        <items id="description">Pathological staging is performed by postmortem examination after the death of the patient.</items>
      </items>
      <items code="at0021">
        <items id="text">TNM Edition</items>
        <items id="description">The edition of the TNM classification system used for the assessment.</items>
      </items>
      <items code="at0022">
        <items id="text">Extension</items>
        <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
        <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
      </items>
      <items code="at0023">
        <items id="text">Perineural invasion (Pn)</items>
        <items id="description">Pathological assessment of invasion into nerve sheaths.</items>
      </items>
      <items code="at0024">
        <items id="text">Multiple primary tumours (m)</items>
        <items id="description">Presence of multiple primary tumours at a single site.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

